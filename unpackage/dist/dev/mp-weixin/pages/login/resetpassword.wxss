
.reset{
	height: 3rem;
	background-color: #f5f6f6;
	text-align: center;
	font-size: 25px;
	line-height: 3rem;
}
.box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 80%;
	height: 40px;
	margin-top: 2rem;
	border: solid;
	margin: 15rpx auto;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background-color: #f5f6f6;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
}
.text{
	margin-top: 2rem;
	margin-left: 10%;
	font-size: 20px;
}
.done{
	height: 3rem;
	width: 80%;
	background-color: #5280c1;
	-webkit-border-radius: 8px;
	        border-radius: 8px;
	font-size: 25px;
	text-align: center;
	margin: 15rpx auto;
	line-height: 3rem;
	margin-top: 2rem;
	color: #FFFFFF;
}
.message{
	margin-top: 21rem;
	margin-left: 15%;
	color: #a2a3a3;
}
.ziti {
	color: black;
	font-weight: 700;
}
.iconbox {
	width: 60rpx;
	height: 60rpx;
	border-right: 1px solid;
}
.iconfont {
	width: 60rpx;
	height: 60rpx;
	font-size: 60rpx;
}


